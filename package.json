{
  "name": "vault_ui",
  "version": "1.0.0",
  "description": "Graphical interface for Hashicorp Vault",
  "main": "main.js",
  "scripts": {
    "pack": "build --dir",
    "dist": "build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon ./server.js --exec babel-node --presets es2015,stage-2",
    "serve": "nodemon ./server.js",
    "build-desktop": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.desktop.config.js --progress --profile --colors",
    "build-web": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.min.config.js --progress --profile --colors",
    "run-desktop": "electron .",
    "package": "npm run build-desktop && build --publish never",
    "package-win64": "npm run build-desktop && build --win --x64",
    "package-win32": "npm run build-desktop && build --win --ia32",
    "package-linux": "npm run build-desktop && build --linux",
    "package-all": "npm run build-desktop && build -mwl",
    "cleanup": "mop -v"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djenriquez/vault-ui.git"
  },
  "keywords": [],
  "author": {
    "name": "Vault UI Contributors",
    "email": "no-reply@vault-ui.djenriquez.github.com"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/djenriquez/vault-ui/issues"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-2"
    ]
  },
  "homepage": "https://github.com/djenriquez/vault-ui#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "css-loader": "^0.25.0",
    "electron": "^1.4.1",
    "electron-builder": "^13.3.2",
    "eslint": "^3.14.0",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "json-loader": "^0.5.4",
    "material-ui": "^0.16.4",
    "nodemon": "^1.11.0",
    "postcss-loader": "^1.1.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "autoprefixer": "^6.5.3",
    "axios": "^0.15.2",
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "copy-to-clipboard": "^3.0.5",
    "cross-env": "^3.1.4",
    "express": "^4.14.0",
    "hbs": "^4.0.1",
    "hcl-to-json": "0.0.4",
    "immutability-helper": "^2.1.2",
    "jsoneditor": "^5.5.11",
    "lodash": "^4.16.6",
    "material-ui": "^0.16.1",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-router": "^3.0.0",
    "react-tap-event-plugin": "^2.0.0",
    "react-ultimate-pagination-material-ui": "^0.5.0"
  },
  "build": {
    "productName": "Vault-UI",
    "appId": "com.github.djenriquez.vault-ui",
    "mac": {
      "category": "public.app-category.tools"
    },
    "protocols": [
      {
        "name" : "Vault-UI Desktop URL Protocol Handler",
        "schemes": [ "vaultui" ]
      }
    ],
    "dmg": {
      "icon" : "build/icon.icns",
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "files": [
      "dist/",
      "appindex.html",
      "main.js",
      "package.json"
    ],
    "win": {
      "icon" : "build/icon.ico",
      "target": "nsis"
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ]
    },
    "directories": {
      "buildResources": "build",
      "output": "release"
    }
  }
}
